<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2" hint="target=sky">
% for object in objects:
    <Placemark>
        <name>${object.peak}</name>
        <Point>
            <coordinates>${object.ra - 180}, ${object.dec}</coordinates>
        </Point>
    </Placemark>
% endfor
</kml>
