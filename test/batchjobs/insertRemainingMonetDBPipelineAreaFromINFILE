#!/bin/sh
# Input args: database (mysql/monetdb), writeToFile (true/false),
# Nimages, Nbands, Nbeams, Nsources (per image)

for (( j = 0 ; j < 1; j++ ))
do
 echo "iter: `expr $j`"
 ../databases/MonetDB/bin/MapiClient -lsql -umonetdb -Pmonetdb < ../databases/design/sql/create.pipelineMonetDBNoINDX.sql
 java -Xmx1000M org/lofar/pipeline/InsertPipelineArea monetdb true 10000 10 10 400 
done

